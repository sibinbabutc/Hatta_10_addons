<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_view_general_configuration" model="ir.ui.view">
        <field name="name">General Settings</field>
        <field name="model">base.config.settings</field>
        <field name="inherit_id" ref="base_setup.view_general_configuration"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='company_share_resources']" position="after">
                <group>
                    <field class="oe_avtar" name="fav_icon_backend" widget="image"/>
                    <field class="oe_avtar" name="backend_logo" widget="image"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>