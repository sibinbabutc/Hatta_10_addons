<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="hatta_partner_tax_report_view" model="ir.ui.view">
        <field name="name">res.partner.inherit.form.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="hatta_trading.hatta_partner_view"/>
        <field name="arch" type="xml">
            <xpath expr='//field[@name="vat"]' position="after">
                <field name="tax_state"/>
            </xpath>
        </field>
    </record>
</odoo>